function h(bool x) -> int {
    int value;
    if (x) value = 5;
    else value = 10;
    return value;
}

function spill_test() -> int
{
    int x = 5;
    int y = 2*x;
    int z = y - 2;
    int w = x + z;

    int a = x*x + y*y + z*z + w*w;
    int b = a + x*y - z*w;

    return a - b; // 54
}

